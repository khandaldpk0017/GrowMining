          .col-span-12
            .card.buttons
              .card-header
                h4 pagination alignment
              .card-body
                nav.my-3
                  ul.pagination
                    li.page-item.disabled
                      a.page-link(href='#' tabindex='-1') Previous
                    li.page-item
                      a.page-link(href='#') 1
                    li.page-item
                      a.page-link(href='#') 2
                    li.page-item
                      a.page-link(href='#') 3
                    li.page-item
                      a.page-link(href='#') Next
                nav.my-3
                  ul.pagination.justify-center
                    li.page-item.disabled
                      a.page-link(href='#' tabindex='-1') Previous
                    li.page-item
