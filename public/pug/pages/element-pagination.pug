doctype html
html(lang="en")
  include ../components/mixinfiles
  +headerfile("pagination")
  body

    // Loader Start
    +loader
    // Loader End

    // Header Start
    +header
    // Header End


    .themebody-wrap

      // breadcrumb start
      +codexbreadcrumb('pagination','element')
      // breadcrumb end

      // theme body start
      .theme-body       
        .grid.grid-cols-12.cdx-gap
          .col-span-12(class="2xl:col-span-4 md:col-span-6")
            .card.buttons
              .card-header
                h4 normal pagination
              .card-body
                nav
                  ul.pagination
                    li.page-item
                      a.page-link(href='#') Previous
                    li.page-item
                      a.page-link(href='#') 1
                    li.page-item
                      a.page-link(href='#') 2
                    li.page-item
                      a.page-link(href='#') 3
                    li.page-item
                      a.page-link(href='#') Next
          .col-span-12(class="2xl:col-span-4 md:col-span-6")            
            .card.buttons
              .card-header
                h4 pagination with icon
              .card-body
                nav
                  ul.pagination
                    li.page-item
                      a.page-link(href='#')
                        i.fa.fa-angle-double-left
                    li.page-item
                      a.page-link(href='#') 1
                    li.page-item
                      a.page-link(href='#') 2
                    li.page-item
                      a.page-link(href='#') 3
                    li.page-item
                      a.page-link(href='#')
                        i.fa.fa-angle-double-right
          .col-span-12(class="2xl:col-span-4 md:col-span-6")            
            .card.buttons
              .card-header
                h4 Disabled and active states
              .card-body
                nav
                  ul.pagination
                    li.page-item.disabled
                      a.page-link(href='#' tabindex='-1') Previous
                    li.page-item
                      a.page-link(href='#') 1
                    li.page-item.active(aria-current='page')
                      a.page-link(href='#') 2
                    li.page-item
                      a.page-link(href='#') 3
                    li.page-item
                      a.page-link(href='#') Next
          .col-span-12(class="2xl:col-span-4 md:col-span-6")            
            .card.buttons
              .card-header
                h4 primary pagination
              .card-body
                nav
                  ul.pagination.pagination-primary
                    li.page-item
                      a.page-link(href='#' tabindex='-1') 
                        i.fa.fa-angle-left
                    li.page-item
                      a.page-link(href='#') 1
                    li.page-item.active(aria-current='page')
                      a.page-link(href='#') 2
                    li.page-item
                      a.page-link(href='#') 3
                    li.page-item
                      a.page-link(href='#')
                        i.fa.fa-angle-right
